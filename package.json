{
  "name": "@opensea/pushed-updates-sdk",
  "version": "0.0.1",
  "description": "An SDK to receive pushed updates from OpenSea over websocket",
  "license": "MIT",
  "author": "OpenSea Developers",
  "repository": "https://github.com/ProjectOpenSea/pushed-updates-sdk",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle",
    "check-types": "tsc --noEmit",
    "dev": "microbundle watch",
    "eslint": "eslint . --max-warnings 0 --ext .js,.ts --fix",
    "lint:check": "concurrently \"npm run check-types\" \"npm run eslint\" \"yarn prettier:check\" \"npm run prettier:check:package.json\"",
    "prepare": "husky install",
    "test": "jest",
    "prettier:check": "prettier --check .",
    "prettier:check:package.json": "prettier-package-json --list-different",
    "prettier:fix": "prettier --write ."
  },
  "sideEffects": false,
  "dependencies": {
    "phoenix": "^1.6.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/phoenix": "1.5.4",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "concurrently": "7.0.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-websocket-mock": "2.3.0",
    "lint-staged": "12.3.7",
    "microbundle": "0.14.2",
    "prettier": "2.6.0",
    "prettier-package-json": "2.6.3",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write"
    ],
    "**/*.{ts,tsx,js,jsx,html,md,mdx,yml,json}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix"
    ]
  },
  "source": "src/index.ts",
  "unpkg": "./dist/index.umd.js"
}
